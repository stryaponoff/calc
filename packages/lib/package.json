    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui"
  },
  "devDependencies": {
    "@vitest/coverage-c8": "^0.29.3",
    "@vitest/ui": "^0.29.3",
    "typescript": "^5.0.2",
    "vite": "^4.2.0",
    "vite-plugin-dts": "^2.1.0",
    "vitest": "^0.29.3"
  }
